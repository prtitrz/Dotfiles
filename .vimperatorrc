"2.3.1 (created: 2010/04/07 18:56:53)

set runtimepath=/home/rz/.vimperator
source! /home/rz/.vimperatorrc.local

"set guioptions=B

set nextpattern=\s*下一页|下一张|下一章|下一篇|下页|后页\s*,^\bnext\b,\\bnext\\b,^>$,^(>>|»)$,^(>|»),(>|»)$,\\bmore\\b
set previouspattern=\s*上一页|上一张|上一章|上一篇|上页|前页\s*,^\bprev|previous\b,\\bprev|previous\\b,^<$,^(<<|«)$,^(<|«),(<|«)$

" 此项功能是自动补全.在命令行下的自动补全.
set wop=auto
"命令行下拉菜单最多显示的条目数
set maxitems=6

command! wd -nargs=* tabopen http://dict.cn/<args>
map <silent> wd :exe ":wd "+content.getSelection()<CR>
command! db -nargs=* tabopen http://book.douban.com/subject_search?search_text=<args>&cat=1001
map <silent> db :exe ":db "+content.getSelection()<CR>
command! dm -nargs=* tabopen http://movie.douban.com/subject_search?search_text=<args>&cat=1002
map <silent> dm :exe ":dm "+content.getSelection()<CR>
command! dy -nargs=* tabopen http://music.douban.com/subject_search?search_text=<args>&cat=1003
map <silent> dy :exe ":dy "+content.getSelection()<CR>
command! ss -nargs=* tabopen http://shooter.cn/search/<args>
map <silent> ss :exe ":ss "+content.getSelection()<CR>

let g:maine_coon_default="am"

noremap <C-c> <C-v><C-c>
noremap <C-a> <C-v><C-a>
cnoremap <C-c> <C-v><C-c>
cnoremap <C-v> <C-v><C-v>
cnoremap <C-x> <C-v><C-x>
inoremap <C-a> <C-v><C-a>
inoremap <C-c> <C-v><C-c>
inoremap <C-v> <C-v><C-v>
inoremap <C-x> <C-v><C-x>
inoremap <C-z> <C-v><C-z>
inoremap <C-y> <C-v><C-y>

autocmd LocationChange .* :js modes.passAllKeys = /mail\.google\.com/.test(buffer.URL) || /google\.com\/reader\//.test(buffer.URL) 

noremap dd <C-w>
noremap h <C-d>
noremap l <C-u>
noremap j 7j
noremap k 7k
noremap y Y
noremap Y y
noremap H h
noremap L l
noremap J j
noremap K k
noremap = +
map s :stop<CR>
map <C-k> :tabopen google 
map <C-l> :tabopen 
noremap gr gT
noremap <A-i> <C-i>
noremap <A-o> <C-o>

highlight StatusLine color:#bf783a; background:#d6c6af
highlight CmdLine color:blue; background:#fef4f4


""MigrateStatusbar
""
javascript <<EOM
     liberator.globalVariables.migrate_elements = [
   		 {
         //Readitlater watch-button
             id:    'RIL_toolbar_button',
             dest:  'security-button',
             after: true,
         },
         {
         //Readitlater add-button
             id:    'RIL_urlbar_add',
             dest:  'security-button',
             after: true,
         },
         {
         //Readitlater cancel-button
             id:    'RIL_urlbar_mark',
             dest:  'security-button',
             after: true,
         },
	];
EOM


loadplugins
